    "version": "0.2.0",
    "configurations": [
        {
            "name": "Catebi.Map.WebApi",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/Catebi.Map.WebApi/bin/Debug/net8.0/Catebi.Map.WebApi.dll",
            "args": [],
            "cwd": "${workspaceFolder}/Catebi.Map.WebApi",
            "stopAtEntry": false,
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            },
            "justMyCode": true,
            "logging": {
                "moduleLoad": false
            }
        },
        {
            "name": "Docker .NET Launch",
            "type": "docker",
            "request": "launch",
            "preLaunchTask": "docker-run: debug",
            "netCore": {
                "appProject": "${workspaceFolder}/Catebi.Map.WebApi/Catebi.Map.WebApi.csproj"
            }
        }
    ]
}