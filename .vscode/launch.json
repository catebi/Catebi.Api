    "version": "0.2.0",
    "configurations": [
        {
            "name": "Catebi.Map.WebApi",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/Catebi.Map.WebApi/bin/Debug/net8.0/Catebi.Map.WebApi.dll",
            "args": [],
            "cwd": "${workspaceFolder}/Catebi.Map.WebApi",
            "stopAtEntry": false,
            // "serverReadyAction": {
            //     "action": "openExternally",
            //     "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
            // },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            },
            "justMyCode": true, // You can change to false if you wanna debug 3rd-party code
            "logging": {
              "moduleLoad": false
            }
        }

    ]
